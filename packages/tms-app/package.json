{
  "name": "@han-keong/tms-app",
  "version": "2.0.0",
  "description": "Web service for the Task Management System",
  "license": "ISC",
  "author": "Xie Han Keong",
  "main": "./src/Main.js",
  "scripts": {
    "build": "webpack",
    "dev": "cross-env NODE_ENV=development npm-run-all -s generateHtml start:dev",
    "generateHtml": "babel-node --root-mode upward --config-file ./generateHtml.babelrc generateHtml.js",
    "prod": "cross-env NODE_ENV=production npm-run-all -s generateHtml build start:prod",
    "start:dev": "webpack serve",
    "start:prod": "node previewDist.js"
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@han-keong/tms-helpers": "^2.0.0",
    "@han-keong/tms-types": "^2.0.0",
    "@han-keong/tms-validators": "^2.0.0",
    "axios": "^1.5.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-webpack-alias": "^2.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^8.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.16.0",
    "react-tag-autocomplete": "^7.1.0",
    "react-toggle": "^4.1.3",
    "react-tooltip": "^5.21.5",
    "react-transition-group": "^4.4.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
