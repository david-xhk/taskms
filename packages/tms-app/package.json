    "html-webpack-plugin": "^5.5.3",
    "immer": "^10.0.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-tag-autocomplete": "^7.1.0",
